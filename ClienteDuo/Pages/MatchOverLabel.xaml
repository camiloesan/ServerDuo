<UserControl x:Class="ClienteDuo.Pages.MatchOverLabel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ClienteDuo.Pages"
             xmlns:p="clr-namespace:ClienteDuo.Properties"
             mc:Ignorable="d" 
             d:DesignHeight="200" d:DesignWidth="800" HorizontalAlignment="Center" VerticalAlignment="Center">
    <Grid Background="Black">
        <Label Content="{x:Static p:Resources.LblMatchEnd}" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" Height="120" Width="750" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontFamily="Mario Party Hudson" FontSize="80" FontWeight="Bold" Foreground="White" BorderBrush="Black"/>
        <Label Content="{x:Static p:Resources.LblCalculatingResults}" HorizontalAlignment="Center" Margin="0,140,0,0" VerticalAlignment="Top" Height="50" Width="650" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="40" FontFamily="Mario Party Hudson" Foreground="#FFECE3E3" BorderBrush="Black"/>

    </Grid>
</UserControl>
